<%= link_to '新增會員', new_user_path %>

<table>
  <thead>
    <tr>
      <th>姓名</th>
      <th>Email</th>
      <th>學校分機</th>
	  <th>電話號碼</th>      
      <th>手機號碼</th>
      <th>會員身分</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= showBlank(data: user.extend) %></td>
        <td><%= showBlank(data: user.phone_no) %></td>
        <td><%= showBlank(data: user.mobile_no) %></td>
        <td><%= Hash[identityOptions].rassoc(user.identity).first%> </td>
        <td><%= link_to '詳細', user %></td>
        <!--
        <td><%= link_to '編輯', edit_user_path(user) %></td>
        -->
        <td><%= link_to '刪除', user, method: :delete, data: { confirm: '您確定要刪除會員 '+user.name+' ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


